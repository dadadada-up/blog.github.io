<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 200" width="200" height="200">
  <defs>
    <pattern id="qrPattern" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
      <rect width="20" height="20" fill="white"/>
      <rect x="2" y="2" width="8" height="8" fill="black"/>
      <rect x="12" y="2" width="6" height="6" fill="black"/>
      <rect x="2" y="12" width="6" height="6" fill="black"/>
      <rect x="12" y="12" width="8" height="8" fill="black"/>
    </pattern>
  </defs>
  
  <!-- 背景 -->
  <rect width="200" height="200" fill="white" stroke="#ddd" stroke-width="2"/>
  
  <!-- 二维码图案 -->
  <rect x="20" y="20" width="160" height="160" fill="url(#qrPattern)"/>
  
  <!-- 四个角的定位点 -->
  <rect x="25" y="25" width="30" height="30" fill="black"/>
  <rect x="30" y="30" width="20" height="20" fill="white"/>
  <rect x="35" y="35" width="10" height="10" fill="black"/>
  
  <rect x="145" y="25" width="30" height="30" fill="black"/>
  <rect x="150" y="30" width="20" height="20" fill="white"/>
  <rect x="155" y="35" width="10" height="10" fill="black"/>
  
  <rect x="25" y="145" width="30" height="30" fill="black"/>
  <rect x="30" y="150" width="20" height="20" fill="white"/>
  <rect x="35" y="155" width="10" height="10" fill="black"/>
  
  <!-- 中心图案 -->
  <rect x="85" y="85" width="30" height="30" fill="black"/>
  <rect x="90" y="90" width="20" height="20" fill="white"/>
  <rect x="95" y="95" width="10" height="10" fill="black"/>
  
  <!-- 文字说明 -->
  <text x="100" y="15" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">微信二维码</text>
  <text x="100" y="195" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#999">扫码添加好友</text>
</svg> 